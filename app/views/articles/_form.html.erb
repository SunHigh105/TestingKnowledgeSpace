<%= form_for(@article) do |f| %>
  <%= render 'shared/error_messages', object: f.object %>

  <%= f.label :title %>
  <%= f.text_field :title, class: 'form-control' %>

  <%= f.label :content %>
  <%= f.text_area :content, :rows => '10', class: 'form-control' %>
  <%= f.submit "保存", class: "btn form-btn" %>
<% end %>